<?import java.lang.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.textfield.*?>

   <BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ursign.ursign_client.FXQuizController"
            prefHeight="400.0" prefWidth="600.0"  fx:id="rootBorderPane">
	  <top>
	  <VBox prefHeight="100.0" fx:id="topVbox">
           <children>
               <Separator prefWidth="200.0"   />
               <Label fx:id="message" style="-fx-text-fill: red;"></Label>
               </children>
         </VBox>
	  </top>
   	  <bottom>
		<VBox prefHeight="50.0" BorderPane.alignment="BOTTOM_CENTER">
           <children>
               <Separator prefWidth="200.0" fx:id="extraControlsSep"  />
                <HBox alignment="CENTER" fillHeight="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="40.0" minWidth="400.0" prefHeight="40.0" prefWidth="800.0" spacing="5.0" fx:id="controlbox">
					
                </HBox>
                </children>		   
             </VBox>
	</bottom>
   </BorderPane>
