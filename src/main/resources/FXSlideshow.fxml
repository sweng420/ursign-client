<?import java.lang.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.textfield.*?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ursign.ursign_client.FXSlideshowController"
            prefHeight="400.0" prefWidth="600.0">

   <Label text=" This is Tab 1 " translateX="10"></Label>
   <BorderPane prefHeight="100.0" prefWidth="600.0"  fx:id="rootBorderPane">
   	<bottom>
		<VBox prefHeight="50.0" BorderPane.alignment="BOTTOM_CENTER">
           <children>
               <Separator prefWidth="200.0" fx:id="extraControlsSep"  />
                <HBox alignment="CENTER" fillHeight="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="40.0" minWidth="400.0" prefHeight="40.0" prefWidth="800.0" spacing="5.0" fx:id="gallerybox_controlbox">
				<Button disable="true" fx:id="slide_prevbutton" text="Previous" underline="true" onAction="#handleSlidePrev"></Button>
                <Button disable="true" fx:id="slide_nextbutton" text="Next" underline="true" onAction="#handleSlideNext"></Button>
                </HBox>
                </children>		   
             </VBox>
				</bottom>
   </BorderPane>
   
</AnchorPane>